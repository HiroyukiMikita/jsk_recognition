<!-- -*- mode: xml -*- -->
<launch>
  <param name="display" type="int" value="0"/>
  <node name="sr4000" pkg="swissranger_camera"
	type="swissranger_camera" respawn="false" output="screen"
	clear_params="true" ns="RoomRangeSensor">
    <param name="frame_id" type="string" value="/sr4000_room_cam"/>
    <!-- 15.5MHz / mudulation_freq = 11 -->
    <param name="modulation_freq" value="11"/>
    <param name="auto_exposure" value="0"/>
    <param name="integration_time" value="255"/>
    <!--
    <param name="camera_info_url"
	   value="file:///$(find cr_capture)/launch/2010-04-20-19-14-52-sr4000.yaml"/>
    -->
    <param name="camera_info_url"
	   value="file:///$(find cr_capture)/launch/2010-07-18-14-00-26-sr4000.yaml"/>
    <!-- <param name="ether_addr" value="192.168.101.151"/> -->
    <param name="ether_addr" value="192.168.101.163"/>
    <!-- <remap from="~distance/image_raw16" to="~image_depth"/> -->

  </node>
  <!--
  <node name="iview" pkg="image_view" type="image_view">
    <remap from="image" to="sr4000/intensity/image_raw"/>
  </node>
  -->
</launch>
