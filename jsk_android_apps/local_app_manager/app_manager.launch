<launch>
  <rosparam>
robot:
  name: laptop
  type: turtlebot # only pr2,turtlebot ??
# exchange_url: https://kforge.ros.org/pr2apps/pr2_exchange/raw-file/tip
  </rosparam>

  <node pkg="app_manager" type="appmaster" name="appmaster" args="-p 11312"/>

  <node pkg="app_manager" type="app_manager" name="app_manager" args="--applist $(find local_app_manager)/mock_apps" output="screen" cwd="node">
        <param name="interface_master" value="http://localhost:11312"/>
  </node>
</launch>
