<launch>
  <node name="elevator_number" pkg="elevator_move_base_pr2" type="matchtemplate.py"
	output="screen">
    <param name="template_list" value="B2F B1F 1F 2F 3F 4F 5F 6F 7F"/>
    <param name="template/B2F/name" value="/eng8_b2f"/>
    <param name="template/B2F/path" value="$(find elevatpr_move_base_pr2)/elevator_numbers/b2f.png"/>
    <param name="template/B2F/thre" value="0.05"/>
    <param name="template/B1F/name" value="/eng8_b1f"/>
    <param name="template/B1F/path" value="$(find elevatpr_move_base_pr2)/elevator_numbers/b1f.png"/>
    <param name="template/B1F/thre" value="0.05"/>
    <param name="template/1F/name" value="/eng8_1f"/>
    <param name="template/1F/path" value="$(find elevatpr_move_base_pr2)/elevator_numbers/1f.png"/>
    <param name="template/1F/thre" value="0.05"/>
    <param name="template/2F/name" value="/eng8_2f"/>
    <param name="template/2F/path" value="$(find elevatpr_move_base_pr2)/elevator_numbers/2f.png"/>
    <param name="template/2F/thre" value="0.05"/>
    <param name="template/3F/name" value="/eng8_3f"/>
    <param name="template/3F/path" value="$(find elevatpr_move_base_pr2)/elevator_numbers/3f.png"/>
    <param name="template/3F/thre" value="0.05"/>
    <param name="template/4F/name" value="/eng8_4f"/>
    <param name="template/4F/path" value="$(find elevatpr_move_base_pr2)/elevator_numbers/4f.png"/>
