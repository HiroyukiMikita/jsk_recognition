    <param name="template/5F/path" value="$(find elevatpr_move_base_pr2)/elevator_numbers/5f.png"/>
    <param name="template/5F/thre" value="0.05"/>
    <param name="template/6F/name" value="/eng8_6f"/>
    <param name="template/6F/path" value="$(find elevatpr_move_base_pr2)/elevator_numbers/6f.png"/>
    <param name="template/6F/thre" value="0.05"/>
    <param name="template/7F/name" value="/eng8_7f"/>
    <param name="template/7F/path" value="$(find elevatpr_move_base_pr2)/elevator_numbers/7f.png"/>
    <param name="template/7F/thre" value="0.05"/>

    <remap from="~image" to="/panel_camera/image"/>
  </node>
</launch>
