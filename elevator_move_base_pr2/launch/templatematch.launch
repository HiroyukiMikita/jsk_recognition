<launch>
  <node name="elevator_number" pkg="elevator_move_base_pr2" type="matchtemplate.py"
	output="screen" machine="c2">
    <param name="template_list" value="B2F B1F 1F 2F 3F 4F 5F 6F 7F"/>
    <param name="template/B2F/name" value="b2f"/>
    <param name="template/B2F/path" value="$(find elevator_move_base_pr2)/elevator_numbers/b2f.png"/>
    <param name="template/B2F/thre" value="0.05"/>
    <param name="template/B1F/name" value="b1f"/>
    <param name="template/B1F/path" value="$(find elevator_move_base_pr2)/elevator_numbers/b1f.png"/>
    <param name="template/B1F/thre" value="0.05"/>
    <param name="template/1F/name" value="1f"/>
    <param name="template/1F/path" value="$(find elevator_move_base_pr2)/elevator_numbers/1f.png"/>
    <param name="template/1F/thre" value="0.05"/>
    <param name="template/2F/name" value="2f"/>
    <param name="template/2F/path" value="$(find elevator_move_base_pr2)/elevator_numbers/2f.png"/>
    <param name="template/2F/thre" value="0.05"/>
    <param name="template/3F/name" value="3f"/>
    <param name="template/3F/path" value="$(find elevator_move_base_pr2)/elevator_numbers/3f.png"/>
    <param name="template/3F/thre" value="0.05"/>
    <param name="template/4F/name" value="4f"/>
    <param name="template/4F/path" value="$(find elevator_move_base_pr2)/elevator_numbers/4f.png"/>
    <param name="template/4F/thre" value="0.05"/>
    <param name="template/5F/name" value="5f"/>
    <param name="template/5F/path" value="$(find elevator_move_base_pr2)/elevator_numbers/5f.png"/>
    <param name="template/5F/thre" value="0.05"/>
    <param name="template/6F/name" value="6f"/>
    <param name="template/6F/path" value="$(find elevator_move_base_pr2)/elevator_numbers/6f.png"/>
    <param name="template/6F/thre" value="0.05"/>
    <param name="template/7F/name" value="7f"/>
    <param name="template/7F/path" value="$(find elevator_move_base_pr2)/elevator_numbers/7f.png"/>
    <param name="template/7F/thre" value="0.05"/>

    <remap from="~image" to="/panel_camera/image"/>
  </node>
</launch>
