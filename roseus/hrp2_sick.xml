<launch>

  <group name="wg">
    <param name="sicklms/port" value="/dev/ttyUSB0" />
    <param name="sicklms/baud" value="500000" />
    <param name="sicklms/inverted" value="false"/> 
    <node pkg="sicktoolbox_wrapper" type="sicklms" output="screen" />

    <node pkg="tf" type="transform_sender" args="0.270 0 0.230 0 0 0 base_laser base_link_offset 40" />
    <node pkg="tf" type="transform_sender" args="0 0 0 0 0 0 base_footprint base_link 40" /> 
    <node pkg="joy" type="joy" />
    <node pkg="roseus" type="hrp2v_odom" />
  </group>
</launch>

